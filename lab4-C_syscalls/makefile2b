all: task2b

task2b: lab4_util.o task2b.o lab4_start.o
	ld -m elf_x86_64 lab4_start.o task2b.o lab4_util.o -o task2b

lab4_util.o: lab4_util.c   
	gcc -m64 -Wall -ansi -c -nostdlib -fno-stack-protector lab4_util.c -o lab4_util.o
task2b.o: task2b.c
	gcc -m64 -Wall -ansi -c -nostdlib -fno-stack-protector task2b.c -o task2b.o

lab4_start.o: lab4_start.s
	nasm -f elf64 lab4_start.s -o lab4_start.o

clean: 
	rm -f *.o task2b
